{
  "kind": "collectionType",
  "collectionName": "listings",
  "info": {
    "singularName": "listing",
    "pluralName": "listings",
    "displayName": "Listing"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "cover": {
      "allowedTypes": [
        "images",
        "files"
      ],
      "type": "media",
      "multiple": false,
      "required": true
    },
    "city": {
      "type": "string",
      "required": true
    },
    "blocks": {
      "type": "blocks",
      "required": true
    },
    "images": {
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "type": "media",
      "multiple": true
    },
    "title": {
      "type": "string",
      "required": true
    },
    "floor_plan": {
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "type": "media",
      "multiple": false
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "Mieszkanie",
        "Dom",
        "Działka",
        "Lokal",
        "Inne"
      ],
      "required": true
    },
    "transaction": {
      "type": "enumeration",
      "enum": [
        "Wynajem",
        "Sprzedaż"
      ],
      "required": true,
      "default": "Sprzedaż"
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::author.author"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    }
  }
}
